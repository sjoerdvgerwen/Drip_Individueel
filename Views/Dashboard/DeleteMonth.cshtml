@model Drip.Webapp.Models.MonthModels.DeleteMonthViewModel;




<label for="cars">Which month do you want to delete ? </label>
<form asp-controller="Dashboard" asp-action="DeleteById" type="post">
    <select name="MonthID">
        @foreach (var month in Model.Months)
        {
<option value="@month.MonthID"> @month.NameOfMonth</option>}
    </select>
    <button class="btn btn-danger btn-icon-split" type="submit">SUBMIT</button>
</form>
